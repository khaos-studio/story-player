    {
      "id": "01JKVSP89DHKV1KCCRJ09HAXZ7",
      "label": "D19",
      "projectId": "01JKVSP89B9N5HAWDPV5NTW6JS",
      "type": "dialogue",
      "characterId": "01JFTT916NTSY7CRBP3HWY2H5N",
