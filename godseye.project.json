      ],
      "parentheticals": [
        {
          "text": "(AND KIDS)"
        }
      ]
    },
    {
      "id": "01JKVSP89DEXKHEA06HE3J3X4B",
      "label": "D16",
      "projectId": "01JKVSP89B9N5HAWDPV5NTW6JS",
      "type": "dialogue",
      "characterId": "01JFTT916NRQEMX3CK8KDE8DVY",
