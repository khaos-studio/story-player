    },
    {
      "id": "01JKWBHCXAQ8NJJ5FGE62KSS0A",
      "label": "D18",
      "projectId": "01JKWBHCX80JW1GRE29VS143HC",
      "type": "dialogue",
      "characterId": "01JG5J2ENAE753P2SCPRT7B3V2",
