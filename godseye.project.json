    },
    {
      "id": "01JKVSP89D2WR41CSYG1V20R38",
      "label": "D15",
      "projectId": "01JKVSP89B9N5HAWDPV5NTW6JS",
      "type": "dialogue",
      "characterId": "01JFTT916NVYPRV879FKHCYFRY",
      "lines": [
        {
          "text": "Ya-Hoo!! <laughter, giggles>"
