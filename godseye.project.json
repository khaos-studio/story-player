    },
    {
      "id": "01JKVSP89DSCFZ83YN5ENZM72S",
      "label": "D17",
      "projectId": "01JKVSP89B9N5HAWDPV5NTW6JS",
      "type": "dialogue",
      "characterId": "01JFTT916NW0M8W99ZFVY2P7XM",
      "lines": [
        {
          "text": "Oh, yeah! So what you got for us,\nKevin Hart?"
