    },
    {
      "id": "01JKWBHCXAAY0FAEK78CBKYT9W",
      "label": "D19",
      "projectId": "01JKWBHCX80JW1GRE29VS143HC",
      "type": "dialogue",
      "characterId": "01JG5J2ENA1YS9HW9R4JTNQXYS",
