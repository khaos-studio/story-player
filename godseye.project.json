    },
    {
      "id": "01JKXYNFKPM325XRFZTAZ6AGG9",
      "label": "D15",
      "projectId": "01JKXYNFKNXF0A7TF2BY7G0SGR",
      "type": "dialogue",
      "characterId": "01JG5J2ENA3N6R3G2RQ1QS7D08",
      "lines": [
        {
          "text": "Ya-Hoo!! <laughter, giggles>"
        }
      ],
      "parentheticals": [
        {
          "text": "(AND KIDS)"
        }
      ]
    },
    {
      "id": "01JKXYNFKPF7ZWXKKKEYNQ9B3K",
      "label": "D16",
      "projectId": "01JKXYNFKNXF0A7TF2BY7G0SGR",
      "type": "dialogue",
      "characterId": "01JG5J2ENAQFJ3SKRYZ90TFR4D",
      "lines": [
        {
          "text": "Oh, brother! Corny!"
