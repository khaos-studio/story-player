      "parentheticals": []
    },
    {
      "id": "01JKVR86YD2QKBHH43DDBVANTS",
      "label": "D15",
      "projectId": "01JKVR86YAD8ACXS2V4WYFSJHG",
      "type": "dialogue",
      "characterId": "01JFTT916NVYPRV879FKHCYFRY",
      "lines": [
        {
          "text": "Ya-Hoo!! <laughter, giggles>"
