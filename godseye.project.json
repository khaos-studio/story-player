    {
      "id": "01JKVSP89C5HX5EZTKQWM8W5A2",
      "label": "A1",
      "type": "action",
      "text": "The Seven Grandfather's rockface mountain formation looms on the sunlit horizon beyond the school-",
      "projectId": "01JKVSP89B9N5HAWDPV5NTW6JS"
    },
    {
      "id": "S2",
      "sceneHeading": "INT - SCHOOL CLASSROOM - MORNING",
      "projectId": "01JKVSP89B9N5HAWDPV5NTW6JS",
